     <script> 
        const layerPopupOpen = (t) => {
        let length = 0;
        let layerPopup = document.querySelectorAll(".layer_popup");
        let target = document.querySelector(`.layer_popup.${t}`);

        layerPopup.forEach((target) => {
            if (target.style.display == "block") length++;
        });

        if (length >= 1) {
            let getComputedStyle = window.getComputedStyle(target);
            let zIndex = getComputedStyle.getPropertyValue("z-index");
            target.style.zIndex = Number(zIndex) + length;
        }

        target.style.transition = "opacity 0.2s";
        target.style.opacity = "0";
        target.style.display = "block";
        setTimeout(() => {
            target.style.opacity = "1";
        }, 400);
        };

        // 팝업 닫기: layerPopupClose(타켓 클래스 or 타켓 없을 시 모든 팝업 닫기)
        const layerPopupClose = (t) => {
        if (!t) {
            let layerPopup = document.querySelectorAll(".layer_popup");
            layerPopup.forEach((target) => {
            if (target.style.display == "block") {
                target.style.transition = "opacity 0.2s";
                target.style.opacity = "0";
                setTimeout(() => {
                target.style.display = "none";
                target.removeAttribute("style");
                }, 400);
            }
            });
        } else {
            let target = document.querySelector(`.layer_popup.${t}`);
            target.style.transition = "opacity 0.2s";
            target.style.opacity = "0";
            setTimeout(() => {
            target.style.display = "none";
            target.removeAttribute("style");
            }, 400);
        }
        };

        function fnGetNotice() {
            $.ajax({
                type: "GET",
                contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                url: '/api/eventnotice/241205SQDM',
                dataType: 'json',
                data: {},
                success: function (data) {
                    if (data.resultCode == 0) {
                        $("#NoticeContentsArea").html(data.resultValue);
                        $("#NoticeTitleArea").html(`${data.resultMessage}`);
                        layerPopupOpen('notice');
                    }
                }
            });
        }
    </script>
    

